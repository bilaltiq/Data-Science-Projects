library(mosaic)
library(tidyverse)
library(mdsr)
data(iris)
ggplot(iris, aes(x = Petal.Length, y = Sepal.Length, color = Species))+
geom_point()+
geom_smooth(method = 'lm', se = FALSE)+
labs(y = "Sepal Length (mm)",
x = "Petal Length (mm)",
title = "Scatterplot of Sepal and Petal Lengths",
subtitle = "For all 3 iris Species")
