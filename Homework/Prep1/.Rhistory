data(CIACountries)
# define the plot object
g <- ggplot(data=CIACountries,aes(y=gdp,x=educ))
# print the plot
g + geom_point(aes(color=net_users),size=3)
data(CIACountries)
g <- ggplot(data=CIACountries,aes(y=gdp,x=educ))
g + geom_point(aes(color=net_users),size=3) +
scale_y_continuous(name= "Gross Domestic Product (GDP)") +
scale_x_continuous(name= "% of GDP spent on education")
data(CIACountries)
g <- ggplot(data=CIACountries,aes(y=gdp,x=educ))
g + geom_point(aes(color=net_users),size=3) +
scale_y_continuous(name= "Gross Domestic Product (GDP)") +
scale_x_continuous(name= "% of GDP spent on education") +
theme(legend.position = "top")
data(CIACountries)
g <- ggplot(data=CIACountries,aes(y=gdp,x=educ))
g + geom_point(aes(color=net_users),size=3) +
scale_y_continuous(name= "Gross Domestic Product (GDP)",trans = "log10") +
scale_x_continuous(name= "% of GDP spent on education") +
theme(legend.position = "top")
data(CIACountries)
g <- ggplot(data=CIACountries,aes(y=gdp,x=educ))
g + geom_point(aes(color=net_users),size=3) +
scale_y_continuous(name= "Gross Domestic Product (GDP)",trans = "log10",labels = scales::comma) +
scale_x_continuous(name= "% of GDP spent on education") +
theme(legend.position = "top")
